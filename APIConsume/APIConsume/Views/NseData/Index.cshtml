            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SYMBOL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TIMESTAMP)
            </th>
