@model IEnumerable<APIConsume.Models.SMAT>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<form asp-action="Index" method="post">
    <div>
        <p>

            
            Search SYMBOL: <input type="text" name="searchValue" value="" />
            @*Search TIMESTAMP: <input type="text" name="searchValue" value="" />*@


            <input type="Submit" value="Search" />
            <a asp-action="Index"> Back TO ALL Records</a>


        </p>
    </div>
</form>
<table class="table">
    <thead>
